{
    "app":{
        "name": "Personal Delivery Locker",
        "ajax": null,
        "env": "dev",
        "paths":{
            "*":"http://localhost/pdlocker/node/",
            "api": "http://localhost/pdlocker/node/api/",
            "sql": "http://localhost/pdlocker/node/model/sql/"
        }
    },
    "mods":{
        "webServer":{
            "mod":"web",
            "allowOrigin":"*",
            "delimiter":["&"],
            "uploadWL":[],
            "port":"/tmp/pdl.sock"
        },
        "mainDB":{
            "mod":"mysql",
            "host":"localhost",
            "port":3306,
            "user":"root",
            "password":"root",
            "database":"pdlocker"
        },
        "mainCache":{
            "mod":"redis",
            "host":"localhost",
            "port":6379,
            "database":10,
            "options":{}
        }
    }
}
