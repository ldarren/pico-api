{
    "app":{
        "paths":{
            "*":"http://localhost/sirocco/node/",
            "api": "http://localhost/sirocco/node/api/",
            "redis": "http://localhost/sirocco/node/model/redis/",
            "tpl": "http://localhost/sirocco/node/tpl/"
        }
    },
    "mods":{
        "webServer":{
            "mod":"web",
            "allowOrigin":"*",
            "delimiter":["&"],
            "uploadWL":[],
            "port":"/tmp/sirocco.sock"
        },
        "mainCache":{
            "mod":"redis",
            "host":"localhost",
            "port":6379,
            "database":1,
            "options":{}
        }
    }
}
