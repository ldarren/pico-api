{
    "app":{
        "name": "Ceilia",
        "ajax": null,
        "env": "dev",
        "paths":{
            "*":"http://localhost/ceilia/node/",
            "api": "http://localhost/ceilia/node/api/",
            "sql": "http://localhost/ceilia/node/model/sql/",
            "fsdb": "http://localhost/ceilia/node/model/fsdb/"
        }
    },
    "mods":{
        "webServer":{
            "mod":"web",
            "allowOrigin":"*",
            "delimiter":["&"],
            "uploadWL":[],
            "port":"/tmp/ceilia.sock"
        },
        "mainDB":{
            "mod":"mysql",
            "host":"localhost",
            "port":3306,
            "user":"root",
            "password":"root",
            "database":"ceilia"
        },
        "mainCache":{
            "mod":"redis",
            "host":"localhost",
            "port":6379,
            "database":1,
            "options":{}
        },
        "mainFS":{
            "mod":"fsdb",
            "domainDiv":2,
            "root":"/tmp"
        }
    }
}
