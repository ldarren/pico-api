{
    "deps":{
        "util":"./util",
        "web":["context","webServer"],
        "appMgr":["context","appMgr"]
    },
    "routes":{
        "ERR|#msg":[
            [0]
        ],
        "END|#msg":[
            [0]
        ],
        "/pico":[
            ["util","route","req"],
            ["web","parse","req","res"]
        ],
        "pico/add/app":[
            ["util","help"]
        ],
        "sayHello":[
            ["util","sayHello"]
        ],
		"/:appName|#appPath":[
			["appMgr","redirect","req","res"]
        ],
        "#help": [
            ["util","help"]
		]
    }
}
